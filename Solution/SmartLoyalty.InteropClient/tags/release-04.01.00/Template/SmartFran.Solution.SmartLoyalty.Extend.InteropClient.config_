<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
  </appSettings>
  <system.diagnostics>
    <trace autoflush="true" />
    <sources>
      <source name="SmartLoyalty.InteropClient" switchName="SourceSwitch" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <clear />
		  <add name="XmlListener" type="System.Diagnostics.XmlWriterTraceListener" initializeData="..\Log\InteropClient\SmartLoyalty.InteropClient.svclog" />
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="SourceSwitch" value="All" />
    </switches>
  </system.diagnostics>
  <system.serviceModel>
    <bindings>
      <customBinding>
        <binding name="interopClientBindingConfig">
          <transactionFlow />
          <security authenticationMode="SecureConversation">
            <secureConversationBootstrap authenticationMode="UserNameForSslNegotiated">
              <localClientSettings maxClockSkew="02:00:00" />
            </secureConversationBootstrap>
            <localClientSettings maxClockSkew="02:00:00" />
          </security>
          <textMessageEncoding />
          <httpTransport maxReceivedMessageSize="4194304" maxBufferSize="4194304" maxBufferPoolSize="4194304" />
        </binding>
      </customBinding>
    </bindings>
    <behaviors>
      <endpointBehaviors>
        <behavior name="interopClientBehaviorConfig">
          <clientCredentials>
            <serviceCertificate>
              <authentication certificateValidationMode="None" />
            </serviceCertificate>
          </clientCredentials>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <client>
      <endpoint
      address="http://clubgrido.com.ar:8080/Service/SaleProvider.svc"
      binding="customBinding"
      bindingConfiguration="interopClientBindingConfig"
      behaviorConfiguration="interopClientBehaviorConfig"
      contract="SmartFran.Solution.SmartLoyalty.Service.SaleProviderContext.ISaleProviderApplicationService">
        <identity>
          <certificate encodedValue="" />
        </identity>
      </endpoint>
      <endpoint
      address="http://clubgrido.com.ar:8080/Service/SaleManagementProvider.svc"
      binding="customBinding"
      bindingConfiguration="interopClientBindingConfig"
      behaviorConfiguration="interopClientBehaviorConfig"
      contract="SmartFran.Solution.SmartLoyalty.Service.SaleProviderContext.ISaleManagementProviderApplicationService">
        <identity>
          <certificate encodedValue="" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>